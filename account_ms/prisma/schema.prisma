generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{
  userID      Int       @id @default(autoincrement())
  firstName   String    @db.VarChar(50)
  lastName    String    @db.VarChar(50)
  email       String    @unique
  password    String
  photo       String
}
